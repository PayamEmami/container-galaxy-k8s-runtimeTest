<?xml version='1.0' encoding='UTF-8'?>
<!--Proposed Tool Section: [Utilities]-->
<tool id="mzmatchtoxcms" name="mzmatch-mzmatchtoxcms" version="1.1">
<!--
  <requirements>
    <container type="docker">phnmnl/xcms</container>
  </requirements>
-->
  <description>Convert PeakML to xcmsSet</description>
  <command><![CDATA[
/usr/local/bin/mzmatchtoxcms.r input=$infile output=$outfile
  ]]></command>
  <inputs>
    <param name="infile" type="data" format="xml" value="" optional="False" multiple="True" label="Input PeakML data" />
  </inputs>
  <outputs>
    <data name="outfile" format="rdata"/>
  </outputs>
  <help>Set phenotype of xcmsSet.</help>
</tool>
