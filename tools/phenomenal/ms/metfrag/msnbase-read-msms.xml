<?xml version='1.0' encoding='UTF-8'?>
<!--This is a configuration file for the integration of a tools into Galaxy (https://galaxyproject.org/).-->
<!--Proposed Tool Section: [PhenoMeNal]-->
<tool id="msnbase-read-msms" name="msnbase-read-msms" version="0.1">
  <description>Read MS/MS spectra from mzML file(s).</description>
  <command><![CDATA[
	/usr/local/bin/readMS2MSnBase.r input=$msnbase_read_msms_input_1 output=$msnbase_read_msms_output_1;
  ]]>
  </command>
  <inputs>
    <param name="msnbase_read_msms_input_1" type="data" format="mzml" optional="False" label="Input mzML file" help="Input mzML file containing MS2 information" />
  </inputs>
  <outputs>
    <data name="msnbase_read_msms_output_1" type="data" format="rdata" label="msnbase-read-msms data" />
  </outputs>
  <help>.</help>
</tool>
