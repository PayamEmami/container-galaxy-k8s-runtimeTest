<?xml version='1.0' encoding='UTF-8'?>
<!--This is a configuration file for the integration of a tools into Galaxy (https://galaxyproject.org/).-->
<!--Proposed Tool Section: [PhenoMeNal]-->
<tool id="unzip-collection" name="unZip Data Collection" version="0.1">
  <description>unzip a zip archive.</description>
  <command><![CDATA[
    unzip -p $input > $output;
  ]]>
  </command>
  <inputs>
    <param name="input" type="data" format="zip" optional="False" label="gz data" />
  </inputs>
  <outputs>
    <data name="output" type="data" format="CDF" label="CDF data" />
  </outputs>
  <help>.</help>
</tool>
